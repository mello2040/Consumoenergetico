{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["id", "qtdConsumo", "data", "unidade"],
  "properties": {
    "id": { "type": "integer" },
    "qtdConsumo": { "type": "number", "minimum": 0 },
    "data": { "type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}$" },
    "unidade": { "type": "string", "minLength": 1 }
  },
  "additionalProperties": true
}
